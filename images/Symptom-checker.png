    function getClipData(id) {
        var __url = '/api/?path=video/list-one';
        var __method = 'GET';
        var __data = [];


        $.ajax({
            url: __url,
            type: __method,
            data: {
                "params":'{"id":'+id+'}'
            },
        }).success(function(response){
            // response = JSON.parse(response);
            // $('.modal-edit-clip').show();
            clip_data = response;
            document.getElementById("vc_id").value = id;
            document.getElementById("vc_title").value = response['title'];
            document.getElementById("vc_desc").value = response['desc'];
            document.getElementById("srcMp4").src = response['mp4'];
            document.getElementById("srcWebm").src = response['webm'];
            // document.getElementById("video").poster = response['num'];
            document.getElementById('videoEdit').setAttribute('poster', response['num']);
            document.getElementById('videoEdit').load();

            if(response['vc_is_editorial'] == 1)
              document.getElementById("vc_is_editorial").click();
            if(response['vc_promoted_main'] == 1)
              document.getElementById("vc_promoted_main").click();

            i = 0;
            var promoted = '';

            response['tags'].forEach(function(currentValue, index, arr){
                promoted = currentValue['promoted'] == 0 ? "badge-info" : "badge-primary";
                $('<span class="badge badge-info '+ promoted +'">'+currentValue['name']+'<i class="fa fa-times delete-tag" aria-hidden="true"></i></span>').insertBefore($("#vc_tags"));
            }) 

            initTags();
            initCollections(response['collections']);

            if(response['grade'] > 5)
              response['grade'] = 5;

            $('#star'+response['grade']).attr('checked', 'checked');

        });
    }



    $('#edit_metadata_form').on('submit', function(e){
        e.preventDefault();
        $('.status-wrapper.error').hide();
        var __url = '/api/?path=video/update-clip-data';
        var __method = 'POST';
        var data_aux = $("#edit_metadata_form").serializeArray();
        // var __data = $("#edit_metadata_form input").not('#vc_collections').serialize()
        var __data = [];
        var coll_aux = '';
        var len = 0;
        var promoted_main = false;
        var editorial = false;

        for(i = 0; i < data_aux.length; i++) {
          if(data_aux[i].name == 'vc_tags')
            continue;
          if(data_aux[i].name == 'vc_collections') {
            coll_aux += data_aux[i].value+',';
          } else {
            __data[len] = {};
            __data[len].name = data_aux[i].name;

            if(data_aux[i].name == 'vc_promoted_main' ){
              __data[len].value = 1;
              promoted_main = true;
            } else if (data_aux[i].name == 'vc_is_editorial'){
              __data[len].value = 1;
              editorial = true;
            } else
              __data[len].value = data_aux[i].value ;

            len++;
          }
          
        }

        __data[len] = {};
        __data[len].name = 'vc_collections';
        __data[len].value = coll_aux;

        var tags = '';
        $(".badge-info").each(function(i, obj) {
          if($(this).text() != ' ')
            tags += $(this).text() + ', ';
        });

        len++;
        __data[len] = {};
        __data[len].name = 'vc_tags';
        __data[len].value = tags;


        var vc_promoted_category = '';
        $(".badge-primary").each(function(i, obj) {
          vc_promoted_category += $(this).text() + ', ';
        });

        len++;
        __data[len] = {};
        __data[len].name = 'vc_promoted_category';
        __data[len].value = vc_promoted_category;

        if(promoted_main == false) {
          len++;
          __data[len] = {};
          __data[len].name = 'vc_promoted_main';
          __data[len].value = 0;
        }

        if(editorial == false) {
          len++;
          __data[len] = {};
          __data[len].name = 'vc_is_editorial';
          __data[len].value = 0;
        }

        $.ajax({
            url: __url,
            type: __method,
            data: __data,
        }).success(function(response){
            window.location.reload(false); 
        });
    })

