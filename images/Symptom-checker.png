  <script>

  function removeTags() {
    tags = $('.badge');
    for(i = 0; i < tags.length; i++) {
      tags[i].remove();
    }

    collections = $('.coll_option')
    for(i = 0; i < collections.length; i++) {
      collections[i].remove();
    }

    $("#vc_is_editorial").prop('checked', false).parent().removeClass('active');
    $("#vc_promoted_main").prop('checked', false).parent().removeClass('active');
    $('.modal-edit-clip').hide();
  }

  $( "body" ).on("click", ".edit_clip", function() {
    $('.modal-edit-clip').show();
  });
  $( ".close-modal-edit-clip" ).click(removeTags);
  $( ".cancel-btn-on-modal-edit" ).click(removeTags);
  </script>

